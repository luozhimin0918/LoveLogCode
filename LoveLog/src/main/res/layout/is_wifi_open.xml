<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    
    <FrameLayout
        android:layout_margin="@dimen/space_5"
        android:id="@+id/popuWindowDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         >
         <LinearLayout
             android:gravity="right"
             android:layout_width="fill_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal">
             <ImageView
                 android:id="@+id/integral01"
                 android:layout_marginRight="@dimen/space_10"
                 android:background="@mipmap/integral01"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
             <ImageView
                 android:id="@+id/integral02"
                 android:layout_marginTop="@dimen/space_10"
                 android:layout_marginRight="@dimen/space_10"
                 android:background="@mipmap/integral02"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
             <ImageView
                 android:id="@+id/integral03"
                 android:layout_marginTop="@dimen/space_25"
                 android:layout_marginRight="@dimen/space_15"
                 android:background="@mipmap/integral03"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />


         </LinearLayout>
         
          

    </FrameLayout>

</LinearLayout>